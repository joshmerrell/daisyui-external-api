---
// Base Layout
import BaseLayout from "../layouts/BaseLayout.astro";
import Fieldset from "../components/Fieldset.astro";
import "../styles/myCustomStyles.css";
import { getAbilityScores } from "../getData";

const abilityScoresJSON = await getAbilityScores();
---

<BaseLayout>
	{
		abilityScoresJSON.map((data) => {
			return (
				<Fieldset
					data={data}
					index={data.index}
					desc={data.desc}
					full_name={data.full_name}
					skills={data.skills}
				/>
			);
		})
	}
</BaseLayout>
